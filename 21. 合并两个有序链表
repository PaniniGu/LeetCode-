//解法一：

/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode(int x) : val(x), next(NULL) {}
 * };
 */
class Solution {
public:
    ListNode* mergeTwoLists(ListNode* l1, ListNode* l2) {

        ListNode *result = new ListNode(0);
        ListNode *head = result;
        while(l1 != NULL && l2!=NULL){
            if(l1->val < l2->val){
                result->next = l1;
                l1 = l1->next;
                result = result->next;
            }
            else{
                result->next = l2;
                l2 = l2->next;
                result = result->next;
            }
        }
        if(l1 == NULL){
            result->next = l2;
        }
        if(l2 == NULL){
            result->next = l1;
        }
        
        return head->next;
    }
};


//解法二：

