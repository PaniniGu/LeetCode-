class Solution {
public:
    int maxProfit(vector<int>& prices) {
        if(prices.size() <= 1) return 0;
        
        int n = prices.size();
        int res = 0;
        int premin = prices[0];
        for(int i = 1; i < n; i++){
            if(prices[i] - premin > res) res = prices[i] - premin;
            if(prices[i] < premin) premin = prices[i];
        }
        return res;
    }
};
